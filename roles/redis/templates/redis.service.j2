# {{ ansible_managed }}
[Unit]
Description=Redis persistent key-value database for sessions
After=network.target

[Service]
ExecStart=/usr/bin/redis-server /etc/redis-sessions.conf --daemonize no
ExecStop=/usr/libexec/redis-shutdown redis-sessions
User=redis
Group=redis
RuntimeDirectory=redis
RuntimeDirectoryMode=0755

[Install]
WantedBy=multi-user.target

